{
	"compilerOptions": {
		/* Base Options: */
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleDetection": "force",
		"isolatedModules": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"module": "NodeNext",
		"noEmit": true,
		"lib": ["es2022"],
		"plugins": [
			{
				"name": "@effect/language-service"
			}
		]
	}
}
